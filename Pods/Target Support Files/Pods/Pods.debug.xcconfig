GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/FormatterKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/HackerNewsKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/JHChainableAnimations.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/OCMock.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SFAdditions.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/TTTAttributedLabel.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "FormatterKit" -framework "HackerNewsKit" -framework "JHChainableAnimations" -framework "OCMock" -framework "SFAdditions" -framework "TTTAttributedLabel"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods
PODS_ROOT = ${SRCROOT}/Pods